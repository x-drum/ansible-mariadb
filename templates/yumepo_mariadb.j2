[mariadb]
name = MariaDB
baseurl = http://yum.mariadb.org/{{ mariadb_version }}/{{ ansible_distribution|lower }}{{ ansible_distribution_major_version }}-{% if ansible_architecture == 'x86_64' %}amd64{% else %}i386{% endif %} 
gpgkey=https://yum.mariadb.org/RPM-GPG-KEY-MariaDB
gpgcheck=1
